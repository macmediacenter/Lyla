<window id="6">
<defaultcontrol always="true">50</defaultcontrol>
<allowoverlay>yes</allowoverlay>
<views>501</views>

<controls>

	<include>Background</include>

	<control type="label">
    	<include>WindowID</include>
    	<label>MyVideoNav.xml</label>
		<visible>false</visible>
  	</control>

	<!-- HEADER -->
	<control type="group">
		<posx>MarginLeft</posx>
		
		<control type="label">
			<description>Title Section</description>
			<label>Movies</label>
			<posy>15</posy>
			<width>375</width>
			<height>48</height>
			<include>HeaderTitleFontProperties</include>
			<visible>Container.Content(Movies)</visible>
		</control>
		
		<control type="label">
			<description>SubTitle Section</description>
			<label>All</label>
			<posy>71</posy>
			<width>375</width>
			<height>48</height>
			<include>HeaderSubTitleFontProperties</include>
			<visible>Container.Content(Movies)</visible>
		</control>
	</control>

	<include>WallView</include>
 
